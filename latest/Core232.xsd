      <xsd:documentation xml:lang="ja">始点についての備考を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="始点備考-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="終点" type="ic:地物型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">終点となる地物を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="終点備考" type="ic:終点備考-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">終点についての備考を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="終点備考-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="アクセス区間" type="ic:アクセス区間型" nillable="true">
