      <xsd:documentation xml:lang="ja">時間帯を記述するためのプロパティ用語　※協定世界時からの時間差</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="時間帯-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="カレンダー" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">カレンダーの種類を表すコードを記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="標準型日付" type="ic:標準型日付-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">日付を記述するためのプロパティ用語　※XMLのxsd:date型に準拠したもの</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="標準型日付-単純型">
    <xsd:restriction base="xsd:date"/>
  </xsd:simpleType>
  <xsd:element name="開始日時" type="ic:日時型" nillable="true">
