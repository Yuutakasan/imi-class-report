      <xsd:documentation xml:lang="ja">説明を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="説明-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="使用方法" type="ic:使用方法-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">使用方法の説明を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="使用方法-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
