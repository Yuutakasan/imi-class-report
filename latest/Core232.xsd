      <xsd:element ref="ic:年" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:月" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:日" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:時間帯" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:カレンダー" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:表記" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="期間型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">期間を表現するためのクラス用語</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:期間スケジュール型">
        <xsd:sequence>
          <xsd:group ref="ic:期間型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="期間型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:開始日時" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:終了日時" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:説明" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="期間スケジュール型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">期間やスケジュールを表現するためのクラス用語</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:事物型"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="イベントスケジュール型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">イベントスケジュールを表現するためのクラス用語</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:期間スケジュール型">
        <xsd:sequence>
          <xsd:group ref="ic:イベントスケジュール型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="イベントスケジュール型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:開催日" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:開始日" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:終了日" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:開始時間" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:終了時間" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:説明" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="定期スケジュール型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">定期的に繰り返されるスケジュールを表現するクラス用語</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:期間スケジュール型">
        <xsd:sequence>
          <xsd:group ref="ic:定期スケジュール型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="定期スケジュール型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:種別" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:開催期日" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:有効開始日時" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:有効終了日時" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:開始時間" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:終了時間" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:説明" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="詳細スケジュール型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">（将来的に整備する予定）
施設等のオープン日とオープン時間を表現するためのクラス用語。複数のスケジュールが列挙された場合はそれらの和を取るものとする</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:期間スケジュール型"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="詳細スケジュール規則型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">（将来的に整備する予定）
繰り返し型のスケジュールを定義するためのクラス用語</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:期間スケジュール型"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="コード型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">コードを表現するためのクラス用語</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:事物型">
        <xsd:sequence>
          <xsd:group ref="ic:コード型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="コード型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:コード種別" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:識別値" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:表記" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:上位コード" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:下位コード" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:関連コード" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="単位コード型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">単位コードを表現するためのクラス用語</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:コード型">
        <xsd:sequence>
          <xsd:group ref="ic:単位コード型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="単位コード型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:量種別" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:乗数" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="コードリスト型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">コードリストを表現するためのクラス用語</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:事物型">
        <xsd:sequence>
          <xsd:group ref="ic:コードリスト型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="コードリスト型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:名称" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:発行者" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:バージョン" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:URI" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="アクセス型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">地物等へのアクセスを表現するためのクラス用語</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:事物型">
        <xsd:sequence>
          <xsd:group ref="ic:アクセス型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="アクセス型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:ID" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:種別" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:始点" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:始点備考" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:終点" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:終点備考" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:アクセス区間" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:時間" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:地図" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:備考" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="アクセス区間型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">アクセスに含まれる各区間を表現するためのクラス用語</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:事物型">
        <xsd:sequence>
          <xsd:group ref="ic:アクセス区間型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="アクセス区間型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:種別" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:始点" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:始点備考" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:路線" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:終点" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:終点備考" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:時間" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="事物型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">全ての型のベースとなる基本型としてのクラス用語</xsd:documentation>
    </xsd:annotation>
    <xsd:attribute name="id" type="xsd:ID"/>
    <xsd:attribute name="ref" type="xsd:IDREF"/>
  </xsd:complexType>
  <!--プロパティに対応する要素-->
  <xsd:element name="ID" type="ic:ID型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">事物に割り振られたID（識別子)を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="氏名" type="ic:氏名型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">人の氏名を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="性別" type="ic:性別-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">性別の表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="性別-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="性別コード" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">性別コードを記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="生年月日" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">生年月日を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="死亡年月日" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">死亡年月日を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="住所" type="ic:住所型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">住所を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="本籍" type="ic:住所型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">本籍を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="連絡先" type="ic:連絡先型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">連絡先を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="国籍" type="ic:国籍-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">国籍の表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="国籍-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="国籍コード" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">国籍コードを記述するためのプロパティ用語　※住民基本台帳で利用されているものを使用</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="出生国" type="ic:出生国-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">生まれた国名を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="出生国-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="出生国コード" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">生まれた国のコードを記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="出生地" type="ic:住所型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">生まれた場所を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="種別" type="ic:種別-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">様々な事物の種別の表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="種別-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="表記" type="ic:表記-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">文字列による表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="表記-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="カナ表記" type="ic:カナ表記-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">カタカナ表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="カナ表記-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="ローマ字表記" type="ic:ローマ字表記-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">ローマ字表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="ローマ字表記-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="姓名" type="ic:姓名-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">氏名（姓、名）の表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="姓名-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="姓名カナ表記" type="ic:姓名カナ表記-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">氏名（姓、名）のカタカナ表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="姓名カナ表記-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="姓名ローマ字表記" type="ic:姓名ローマ字表記-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">氏名（姓、名）のローマ字表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="姓名ローマ字表記-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="姓" type="ic:姓-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">氏名の姓の表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="姓-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="姓カナ表記" type="ic:姓カナ表記-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">氏名の姓のカタカナ表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="姓カナ表記-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="姓ローマ字表記" type="ic:姓ローマ字表記-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">氏名の姓のローマ字表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="姓ローマ字表記-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="名" type="ic:名-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">氏名の名の表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="名-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="名カナ表記" type="ic:名カナ表記-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">氏名の名のカタカナ表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="名カナ表記-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="名ローマ字表記" type="ic:名ローマ字表記-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">氏名の名のローマ字表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="名ローマ字表記-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="ミドルネーム" type="ic:ミドルネーム-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">ミドルネームの表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="ミドルネーム-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="ミドルネームカナ表記" type="ic:ミドルネームカナ表記-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">ミドルネームのカタカナ表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="ミドルネームカナ表記-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="ミドルネームローマ字表記" type="ic:ミドルネームローマ字表記-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">ミドルネームのローマ字表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="ミドルネームローマ字表記-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="郵便番号" type="ic:郵便番号-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">郵便番号を記述するためのプロパティ用語　※0-9の数字7桁</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="郵便番号-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="住所コード" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">住所のコードを記述するためのプロパティ用語　※地方公共団体情報システム機構の地方公共団体コード住所など</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="国" type="ic:国-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">国名を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="国-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="国コード" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">国のコードを記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="都道府県" type="ic:都道府県-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">都道府県名を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="都道府県-単純型">
