  <xsd:simpleType name="番地補足-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="番地" type="ic:番地-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">番、街区の表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="番地-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="号" type="ic:号-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">号、住居番号の表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="号-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="ビル名" type="ic:ビル名-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">ビル名を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="ビル名-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="ビル番号" type="ic:ビル番号-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">ビルの番号の表記（号棟など）を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="ビル番号-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="部屋番号" type="ic:部屋番号-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">部屋番号の表記（階数、号室など）を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="部屋番号-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="方書" type="ic:方書-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">方書 (私書箱も含む)を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="方書-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="名称" type="ic:名称型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">名称を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="組織" type="ic:組織型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">組織を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="担当者役職" type="ic:担当者役職-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">担当者の役職の表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="担当者役職-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="担当者名" type="ic:氏名型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">担当者の氏名を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Eメールアドレス" type="ic:Eメールアドレス-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">電子メールアドレスを記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="Eメールアドレス-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="送付先" type="ic:送付先-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">郵便物送付先の住所（私書箱など）を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="送付先-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="電話番号" type="ic:電話番号型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">電話番号を記述するためのプロパティ用語　※0-9 の数字及び + - ( ) , の文字のみを使用</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="内線番号" type="ic:内線番号-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">電話の内線番号を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="内線番号-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="FAX番号" type="ic:電話番号型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">FAX番号を記述するためのプロパティ用語　※0-9 の数字及び + - ( ) , の文字のみを使用 </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="携帯電話番号" type="ic:電話番号型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">携帯電話番号を記述するためのプロパティ用語　※0-9 の数字及び + - ( ) , の文字のみを使用 </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Webサイト" type="ic:Webサイト-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">Webサイトを参照するURLを記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="Webサイト-単純型">
    <xsd:restriction base="xsd:anyURI"/>
  </xsd:simpleType>
  <xsd:element name="対応言語" type="ic:対応言語-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">連絡の際に利用可能な言語名を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="対応言語-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="略称アルファベット" type="ic:略称アルファベット-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">頭文字などを使った略称表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="略称アルファベット-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="通称" type="ic:通称-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">正式な名称以外に一般的に使用されている名称を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="通称-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="活動状況" type="ic:状況型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">組織などの活動状況を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="活動" type="ic:活動型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">組織などの活動を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="概要" type="ic:概要-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">概要説明を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="概要-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="関連組織" type="ic:組織関連型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">関連する組織を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="関連人員" type="ic:人数型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">関連する人員の人数（従業員数など）を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="地物" type="ic:地物型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">関連する地物（施設、建物、土地など）を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="代表者" type="ic:構成員型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">組織の代表者を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="設立日" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">組織などの設立日を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="備考" type="ic:備考-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">備考を記述するためのプロパティ用語　※補足情報などがある場合に記述</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="備考-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="事業種目" type="ic:事業種目-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">事業の種類の表記（販売業など）を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="事業種目-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="事業年度開始日" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">事業年度の開始日を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="代理人" type="ic:組織関連型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">法定手続きなどの代理人となる組織を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="組織種別位置" type="ic:組織種別位置-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">組織名称中の組織種別の記載位置の表記（前、後、など）を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="組織種別位置-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="組織種別" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">組織の種別コード（株式会社など）を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="資本金" type="ic:金額型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">法人などの資本金を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="人数" type="ic:人数-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">人数を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="人数-単純型">
    <xsd:restriction base="xsd:nonNegativeInteger"/>
  </xsd:simpleType>
  <xsd:element name="役割" type="ic:役割-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">人、組織、施設などの役割の表記を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="役割-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="構成員" type="ic:人型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">構成員を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="地理識別子" type="ic:ID型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">場所の地理識別子を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="地理座標" type="ic:座標型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">場所の経緯度座標を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="座標参照系" type="ic:ID型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">座標の参照系を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="緯度経度書式" type="ic:緯度経度書式-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">度、分、秒の書式を記述するためのプロパティ用語　※書式はiso6709に準拠する</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="緯度経度書式-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="緯度" type="ic:緯度-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">緯度を記述するためのプロパティ用語　※-90から+90で記載</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="緯度-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="経度" type="ic:経度-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">経度を記述するためのプロパティ用語　※-180から+180で記載</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="経度-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="座標データ書式" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">点、線、面などを表現するデータ文字列の書式を表すコードを記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="座標データ" type="ic:座標データ-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">点、線、面などを表現するデータ文字列の書式を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="座標データ-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="測地高度" type="ic:長さ型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">測地高度を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="体系" type="ic:ID体系型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">IDの体系を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="識別値" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">IDやコードの値を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
