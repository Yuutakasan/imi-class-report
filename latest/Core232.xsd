  </xsd:simpleType>
  <xsd:element name="金額" type="ic:金額型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">金額を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="発生日" type="ic:日時型" nillable="true">
