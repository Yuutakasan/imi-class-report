  </xsd:element>
  <xsd:simpleType name="標準型日時-単純型">
    <xsd:restriction base="xsd:dateTime"/>
  </xsd:simpleType>
  <xsd:element name="年号" type="ic:年号-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">日付や日時などの元号を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="年号-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="年" type="ic:年-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">日付や日時などの年の値を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="年-単純型">
    <xsd:restriction base="xsd:integer"/>
  </xsd:simpleType>
  <xsd:element name="月" type="ic:月-単純型">
