  <xsd:element name="発行者" type="ic:実体型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">IDやコードリストなどの発行主体を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="バージョン" type="ic:バージョン-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">バージョン情報を記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="バージョン-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="URI" type="ic:URI-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">参照のためのURIを記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="URI-単純型">
    <xsd:restriction base="xsd:anyURI"/>
  </xsd:simpleType>
  <xsd:element name="種別コード" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">種別コードを記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="自動認識ID" type="ic:自動認識ID-単純型">
