  </xsd:element>
  <xsd:element name="容量" type="ic:容量型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">容量を表現するためのクラス用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="実体" type="ic:実体型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">実体を表現するためのクラス用語</xsd:documentation>
