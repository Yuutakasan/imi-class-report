      <xsd:documentation xml:lang="ja">自動認識ID（バーコードや二次元コードの画像など）が表すURLを記述するためのプロパティ用語</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="自動認識ID-単純型">
    <xsd:restriction base="xsd:anyURI"/>
  </xsd:simpleType>
  <xsd:element name="価格" type="ic:価格型" nillable="true">
