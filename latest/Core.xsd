  <xsd:complexType name="日付型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">日付を表現するためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express date.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:概念型">
        <xsd:sequence>
          <xsd:group ref="ic:日付型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="日付型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:標準型日付" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:年号" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:年" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:月" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:日" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:時間帯" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:カレンダー" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:表記" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="期間型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">期間を表現するためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express a period.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:期間スケジュール型">
        <xsd:sequence>
          <xsd:group ref="ic:期間型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="期間型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:開始日時" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:終了日時" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:説明" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="期間スケジュール型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">期間やスケジュールを表現するためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express a period or schedule.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:概念型">
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="イベントスケジュール型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">イベントスケジュールを表現するためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express an event schedule.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:期間スケジュール型">
        <xsd:sequence>
          <xsd:group ref="ic:イベントスケジュール型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="イベントスケジュール型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:開催日" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:開始日" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:終了日" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:開始時間" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:終了時間" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:説明" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="定期スケジュール型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">定期的に繰り返されるスケジュールを表現するクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express a schedule that is repeatedly held on a regular basis.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:期間スケジュール型">
        <xsd:sequence>
          <xsd:group ref="ic:定期スケジュール型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="定期スケジュール型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:開催期日" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:有効開始日時" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:有効終了日時" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:開始時間" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:終了時間" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:説明" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="詳細スケジュール型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">（将来的に整備する予定）施設等のオープン日とオープン時間を表現するためのクラス用語。複数のスケジュールが列挙された場合はそれらの和を取るものとする</xsd:documentation>
      <xsd:documentation xml:lang="en">(To be defined in the future)A class term to express opening time and day of a facility. When multiple schedules are listed, enumerate them.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:期間スケジュール型">
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="詳細スケジュール規則型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">（将来的に整備する予定）繰り返し型のスケジュールを定義するためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">(To be defined in the future) class term to define a repetitive schedule.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:期間スケジュール型">
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="コード型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">コードを表現するためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express a code.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:概念型">
        <xsd:sequence>
          <xsd:group ref="ic:コード型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="コード型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:コード種別" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:識別値" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:表記" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:上位コード" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:下位コード" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:関連コード" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:参照" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="単位コード型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">単位コードを表現するためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express a unit code.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:コード型">
        <xsd:sequence>
          <xsd:group ref="ic:単位コード型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="単位コード型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:量種別" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:乗数" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="コードリスト型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">コードリストを表現するためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express a code list.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:事物型">
        <xsd:sequence>
          <xsd:group ref="ic:コードリスト型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="コードリスト型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:名称" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:発行者" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:バージョン" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="アクセス型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">地物等へのアクセスを表現するためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express a route to a feature, etc.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:概念型">
        <xsd:sequence>
          <xsd:group ref="ic:アクセス型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="アクセス型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:ID" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:始点" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:始点備考" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:終点" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:終点備考" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:アクセス区間" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:時間" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:地図" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:備考" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="アクセス区間型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">アクセスに含まれる各区間を表現するためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express each section of a route.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:概念型">
        <xsd:sequence>
          <xsd:group ref="ic:アクセス区間型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="アクセス区間型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:始点" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:始点備考" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:路線" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:終点" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:終点備考" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:時間" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="概念型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">全ての型のベースとなる基本型としてのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term that serves as the base type for all types.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
        <xsd:group ref="ic:概念型-固有要素グループ"/>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:ID"/>
    <xsd:attribute name="ref" type="xsd:IDREF"/>
  </xsd:complexType>
  <xsd:group name="概念型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:種別" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:種別コード" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:メタデータ" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="事物型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">識別可能なことやものを表すためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to represent an identifiable thing.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:概念型">
        <xsd:sequence>
          <xsd:group ref="ic:事物型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="事物型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:ID" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:表記" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:参照" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:画像" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:説明" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:記述" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="参照型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">追加情報等を参照するためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to refer to additional information.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:概念型">
        <xsd:sequence>
          <xsd:group ref="ic:参照型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="参照型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:参照先" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="記述型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">種別付きの説明文を表現するためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express an explanation in text with a category.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:概念型">
        <xsd:sequence>
          <xsd:group ref="ic:記述型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="記述型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:説明" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="サービス型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">サービスの情報を表現するためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express a service.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:事物型">
        <xsd:sequence>
          <xsd:group ref="ic:サービス型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="サービス型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:名称" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:利用対象" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:提供者" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:利用方法" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="文書型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">文書の情報を表すクラス用語（Dublin Core Metadata Element Setに準拠）</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express information of a document (that complies with Dublin Core Metadata Element Set).</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:事物型">
        <xsd:sequence>
          <xsd:group ref="ic:文書型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="文書型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:表題" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:作成者" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:キーワード" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:発行者" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:寄与者" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:日付" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:資源タイプ" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:記録形式" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:記録形式コード" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:出処" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:言語" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:権利管理" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="対象型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">イベントや制度等の対象となる事物を表すクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express a thing to be targeted for an event or system.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:事物型">
        <xsd:sequence>
          <xsd:group ref="ic:対象型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="対象型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:制約" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="制約型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">対象となる事物の制約条件を表すクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express a restriction on a thing to be targeted.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:概念型">
        <xsd:sequence>
          <xsd:group ref="ic:制約型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="制約型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:制約対象" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:説明" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="コード制約型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">対象となる事物の制約条件をコードによって指定するためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express a restriction on a targeted thing as a code.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:制約型">
        <xsd:sequence>
          <xsd:group ref="ic:コード制約型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="コード制約型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:制約コード" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="範囲制約型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">対象となる事物の制約条件を数量の範囲によって指定するためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express a restriction on a targeted thing by means of a numerical range.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:制約型">
        <xsd:sequence>
          <xsd:group ref="ic:範囲制約型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="範囲制約型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:上限値" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:境界を含まない上限値" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:下限値" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ic:境界を含まない下限値" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="期間制約型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">対象となる事物の制約条件を期間によって指定するためのクラス用語</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express a restriction on a targeted thing by means of a period.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ic:制約型">
        <xsd:sequence>
          <xsd:group ref="ic:期間制約型-固有要素グループ"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="期間制約型-固有要素グループ">
    <xsd:sequence>
      <xsd:element ref="ic:期間" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:group>

  
  <xsd:element name="体系" type="ic:ID体系型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">IDの体系を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The system for specifying identifiers.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ID" type="ic:ID型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">事物に割り振られたID（識別子)を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The identifier assigned to a thing.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="座標参照系" type="ic:ID型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">座標の参照系を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The reference system of latitude-longitude coordinates.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="シリアル番号" type="ic:ID型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">シリアル番号を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The serial number.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="製造者ID" type="ic:ID型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">製造者によって割り当てられたIDを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The identifier assigned by a manufacturer.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="地理識別子" type="ic:ID型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">場所の地理識別子を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The geographical identifier of a location.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="販売業者ID" type="ic:ID型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">販売業者によって割り当てられたIDを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The identifier assigned by a distributor.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="アクセス区間" type="ic:アクセス区間型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">アクセス方法の各区間の一覧を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The list of sections of a route.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="アクセス" type="ic:アクセス型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">アクセス方法を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The route.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="関連イベント" type="ic:イベント型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">関連するイベントを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The related event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="料金" type="ic:価格型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">料金（施設、設備の利用料金やイベントの参加費など）を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The fee (such as a usage fee of a facility or equipment, and a participation fee of an event).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="価格" type="ic:価格型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">価格（製品の価格など）を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The price (such as a product price).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="金額" type="ic:金額型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">金額を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The amount of money.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="資本金" type="ic:金額型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">法人などの資本金を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The capital of a corporation.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="購入価格" type="ic:金額型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">購入価格を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The purchase price.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="代表者" type="ic:構成員型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">組織の代表者を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The representative of an organization.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="座標データ書式" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">点、線、面などを表現するデータ文字列の書式を表すコードを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The code specifying a format of a data string to express points, lines, and planes.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="カレンダー" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">カレンダーの種類を表すコードを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The code specifying a type of calendar.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="国コード" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">国のコードを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The code specifying a country.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="市区町村コード" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">市区町村のコードを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The code specifying a municipality.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="住所コード" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">住所のコードを記述するためのプロパティ用語　※地方公共団体情報システム機構の地方公共団体コード住所など</xsd:documentation>
      <xsd:documentation xml:lang="en">The code specifying an address. Note: Such as Local Government Code Address by Japan Agency for Local Authority Information Systems.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="都道府県コード" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">都道府県のコードを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The code specifying a prefecture.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="コード種別" type="ic:コードリスト型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">コードの種別を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The type of a code.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="量種別" type="ic:量種別-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">量の種別の表記を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The category of a quantity expressed as text.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="量種別-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="乗数" type="ic:乗数-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">基本単位に対する変換乗数を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The conversion multiplier to be applied to the base unit.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="乗数-単純型">
    <xsd:restriction base="xsd:double"/>
  </xsd:simpleType>
  <xsd:element name="原産国コード" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">原産国を表すコードを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The code specifying a country of origin.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="国籍コード" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">国籍コードを記述するためのプロパティ用語　※住民基本台帳で利用されているものを使用</xsd:documentation>
      <xsd:documentation xml:lang="en">The code specifying a nationality. Note: Use the code in Basic Resident Registration.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="種別コード" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">種別コードを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The code specifying a category.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="性別コード" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">性別コードを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The code specifying gender.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="出生国コード" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">生まれた国のコードを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The code specifying a country of birth.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="組織種別" type="ic:コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">組織の種別コード（株式会社など）を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The code specifying the category of an organization (such as a limited corporation).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="施設" type="ic:施設型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">施設を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The facility.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="関連施設" type="ic:施設関連型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">関連する施設（上位施設、下位施設など）を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The associated facility (such as a parent or affiliated facility).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="管理者" type="ic:実体型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">管理している組織や人を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The organization or person in charge of maintenance.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="入数" type="ic:数量型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">箱などに含まれる物の数量を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The quantity of items contained in a box, etc.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="収容人数" type="ic:数量型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">収容人数を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The maximum number of people that a facility can accommodate.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="期間" type="ic:期間スケジュール型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">開始から終了までの時間範囲を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The time range from start to end.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="利用可能時間" type="ic:期間スケジュール型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">利用可能日と利用可能時間を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The available date (or day) and opening hours.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="設備" type="ic:設備型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">設備を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The equipment.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="時間" type="ic:数量型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">所要時間を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The amount of time required.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="設置者" type="ic:組織型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">設備などを設置した組織を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The organization that installed an equipment, etc.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="組織" type="ic:組織型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">組織を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The organization.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="関連組織" type="ic:組織関連型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">関連する組織を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The associated organization.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="代理人" type="ic:組織関連型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">法定手続きなどの代理人となる組織を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The organization that acts as agent for legal procedures.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="建物" type="ic:建物型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">建物を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The building.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="駐車場" type="ic:駐車場型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">駐車場を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The parking area.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FAX番号" type="ic:電話番号型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">FAX番号を記述するためのプロパティ用語　※0-9 の数字及び + - ( ) , の文字のみを使用</xsd:documentation>
      <xsd:documentation xml:lang="en">The facsimile number. Note: Use '0 to 9' numeric characters and 'plus '+', minus '-', opening parenthesis '(', closing parenthesis ')', and comma ',' 'characters only.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="携帯電話番号" type="ic:電話番号型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">携帯電話番号を記述するためのプロパティ用語　※0-9 の数字及び + - ( ) , の文字のみを使用</xsd:documentation>
      <xsd:documentation xml:lang="en">The cellular phone number. Note: Use '0 to 9' numeric characters and 'plus '+', minus '-', opening parenthesis '(', closing parenthesis ')', and comma ',' 'characters only.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="電話番号" type="ic:電話番号型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">電話番号を記述するためのプロパティ用語　※0-9 の数字及び + - ( ) , の文字のみを使用</xsd:documentation>
      <xsd:documentation xml:lang="en">The telephone number. Note: Use '0 to 9' numeric characters and 'plus '+', minus '-', opening parenthesis '(', closing parenthesis ')', and comma ',' 'characters only.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="有効開始日時" type="ic:日時型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">スケジュールが有効になる日時を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The date and time when a schedule becomes effective.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="有効終了日時" type="ic:日時型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">スケジュールが無効になる日時を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The date and time when a schedule becomes ineffective.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="開始日時" type="ic:日時型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">期間の開始日時を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The start date and time of a period.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="終了日時" type="ic:日時型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">期間の終了日時を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The end date and time of a period.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="消費期限" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">消費期限を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The expiration date by which a product should be consumed.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="製造日" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">製造日を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The date of manufacture.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="死亡年月日" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">死亡年月日を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The date of death.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="竣工日" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">建物の竣工日を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The completion date of a building.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="生年月日" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">生年月日を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The date of birth.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="構成員" type="ic:人型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">構成員を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The member.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="名称" type="ic:名称型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">名称を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The name.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="延べ面積" type="ic:面積型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">建物の延べ床面積を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The total floor area of a building.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="建築面積" type="ic:面積型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">建築面積を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The building area.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="敷地面積" type="ic:面積型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">敷地面積を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The ground area.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="連絡先" type="ic:連絡先型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">連絡先を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The contact.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="地理座標" type="ic:座標型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">場所の経緯度座標を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The latitude-longitude coordinates of a location.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="氏名" type="ic:氏名型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">人の氏名を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The name of a person.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="担当者名" type="ic:氏名型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">担当者の氏名を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The name of a contact person.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="関与" type="ic:関与型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">関連する組織や人（関連性の記述を含む）を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The associated organization or person (including a description of the relationship).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="所有者" type="ic:実体型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">所有者を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The owner.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="住所" type="ic:住所型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">住所を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The address.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="本籍" type="ic:住所型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">本籍を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The registered domicile by birth.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="重量" type="ic:重量型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">重量を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The weight.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="場所" type="ic:場所型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">場所を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The location.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="出生地" type="ic:住所型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">生まれた場所を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The birth place.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="活動状況" type="ic:状況型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">組織などの活動状況を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The activity status of an organization, etc.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="開催場所" type="ic:地物型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">イベントなどの開催場所を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The venue of a event, etc.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="地物" type="ic:地物型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">関連する地物（施設、建物、土地など）を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The associated feature (such as a facility, building, or land).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="始点" type="ic:地物型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">始点となる地物を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The feature that serves as an origin.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="終点" type="ic:地物型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">終点となる地物を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The feature that serves as a destination.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="設置地点" type="ic:地物型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">設備などが設置されている地物（施設や建物など）を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The feature in which an equipment is installed. (Such as a facility or building.)</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="測地高度" type="ic:長さ型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">測地高度を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The geodetic height.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="高さ" type="ic:長さ型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">形状の高さを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The height of the shape.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="長さ" type="ic:長さ型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">形状の長さを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The length of the shape.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="幅" type="ic:長さ型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">形状の幅を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The width of the shape.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="最高の高さ" type="ic:長さ型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">最高点の高さを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The height at the highest point.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="発生日" type="ic:日時型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">状況などの発生日時を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The date and time of occurrence of state of affairs, etc.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="開催日" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">イベントなどが開催される日付を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The date on which an event, etc. is held.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="開始日" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">イベントなどの開始日を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The starting date of an event, etc.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="事業年度開始日" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">事業年度の開始日を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The start date of the business year.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="終了日" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">イベントなどの終了日を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The closing date of the event, etc.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="使用期限" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">使用期限を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The expiration date by which a product should be used.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="賞味期限" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">賞味期限を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The best-before-date of a product.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="設置日" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">設備などの設置日を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The installation date of an equipment, etc.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="設立日" type="ic:日付型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">組織などの設立日を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The date of establishment of an organization, etc.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="通貨コード" type="uncefactISO4217:ISO3AlphaCurrencyCodeContentType">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">通貨コードを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The code specifying a currency.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="単位" type="ic:単位コード型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">計測値の単位を表すコードを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The code specifying a unit of measured values.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="単位表記" type="ic:単位表記-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">単位の文字列による表現を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The unit expressed as a string.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="単位表記-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="Webサイト" type="ic:Webサイト-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">Webサイトを参照するURLを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The URL that references a Web site.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="Webサイト-単純型">
    <xsd:restriction base="xsd:anyURI"/>
  </xsd:simpleType>
  <xsd:element name="画像" type="ic:画像-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">画像を参照するURLを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The URL that references an image.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="画像-単純型">
    <xsd:restriction base="xsd:anyURI"/>
  </xsd:simpleType>
  <xsd:element name="参照" type="ic:参照型" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">追加情報などを参照するURLを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The URL that references additional information.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="自動認識ID" type="ic:自動認識ID-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">自動認識ID（バーコードや二次元コードの画像など）が表すURLを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The URL expressed by the automatic recognition identifier (such as an image of bar code and two-dimensional code).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="自動認識ID-単純型">
    <xsd:restriction base="xsd:anyURI"/>
  </xsd:simpleType>
  <xsd:element name="地図" type="ic:地図-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">地図を参照するURLを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The URL that references a map.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="地図-単純型">
    <xsd:restriction base="xsd:anyURI"/>
  </xsd:simpleType>
  <xsd:element name="標準型日付" type="ic:標準型日付-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">日付を記述するためのプロパティ用語　※XMLのxsd:date型に準拠したもの</xsd:documentation>
      <xsd:documentation xml:lang="en">The date. Note: This shall comply with xsd:date type of XML.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="標準型日付-単純型">
    <xsd:restriction base="xsd:date"/>
  </xsd:simpleType>
  <xsd:element name="標準型日時" type="ic:標準型日時-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">日時を記述するためのプロパティ用語　※XMLのxsd:dateTime型に準拠したもの</xsd:documentation>
      <xsd:documentation xml:lang="en">The date and time. Note: This shall comply with xsd:date type of XML.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="標準型日時-単純型">
    <xsd:restriction base="xsd:dateTime"/>
  </xsd:simpleType>
  <xsd:element name="数値" type="ic:数値-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">数値を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The numerical value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="数値-単純型">
    <xsd:restriction base="xsd:decimal"/>
  </xsd:simpleType>
  <xsd:element name="地下階数" type="ic:地下階数-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">建物の地下階数を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The number of underground floors of a building.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="地下階数-単純型">
    <xsd:restriction base="xsd:decimal"/>
  </xsd:simpleType>
  <xsd:element name="地上階数" type="ic:地上階数-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">建物の地上階数を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The number of aboveground floors of a building.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="地上階数-単純型">
    <xsd:restriction base="xsd:decimal"/>
  </xsd:simpleType>
  <xsd:element name="秒" type="ic:秒-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">日時などの秒の値を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The value of second in date and time.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="秒-単純型">
    <xsd:restriction base="xsd:decimal"/>
  </xsd:simpleType>
  <xsd:element name="月" type="ic:月-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">日付や日時などの月の値を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The value of month in date, and date and time.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="月-単純型">
    <xsd:restriction base="xsd:integer"/>
  </xsd:simpleType>
  <xsd:element name="時" type="ic:時-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">日時などの時の値を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The value of time in date and time.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="時-単純型">
    <xsd:restriction base="xsd:integer"/>
  </xsd:simpleType>
  <xsd:element name="収容台数" type="ic:収容台数-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">駐車場の収容可能台数を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The maximum number of vehicles that a parking area can accommodate.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="収容台数-単純型">
    <xsd:restriction base="xsd:integer"/>
  </xsd:simpleType>
  <xsd:element name="年" type="ic:年-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">日付や日時などの年の値を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The value of year in date, and date and time.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="年-単純型">
    <xsd:restriction base="xsd:integer"/>
  </xsd:simpleType>
  <xsd:element name="日" type="ic:日-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">日付や日時などの日の値を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The value of day in date, and date and time.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="日-単純型">
    <xsd:restriction base="xsd:integer"/>
  </xsd:simpleType>
  <xsd:element name="分" type="ic:分-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">日時などの分の値を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The value of minute in date and time.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="分-単純型">
    <xsd:restriction base="xsd:integer"/>
  </xsd:simpleType>
  <xsd:element name="人数" type="ic:人数-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">人数を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The number of people.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="人数-単純型">
    <xsd:restriction base="xsd:nonNegativeInteger"/>
  </xsd:simpleType>
  <xsd:element name="Eメールアドレス" type="ic:Eメールアドレス-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">電子メールアドレスを記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The electronic mail address.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="Eメールアドレス-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="緯度" type="ic:緯度-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">緯度を記述するためのプロパティ用語　※-90から+90で記載</xsd:documentation>
      <xsd:documentation xml:lang="en">The latitude. Note: Specify a value between -90 and +90.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="緯度-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="緯度経度書式" type="ic:緯度経度書式-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">度、分、秒の書式を記述するためのプロパティ用語　※書式はiso6709に準拠する</xsd:documentation>
      <xsd:documentation xml:lang="en">The format to express degree, minutes and seconds. Note: The format shall comply with ISO 6709 standard.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="緯度経度書式-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="色" type="ic:色-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">色の表記を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The color expressed as text.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="色-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="開催期日" type="ic:開催期日-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">スケジュールが有効となる日を記述するためのプロパティ用語　※スケジュールの種別が「週間」の場合は曜日、「月間」の場合は月の中の日、「年間」の場合は年の中の月日を指定する。</xsd:documentation>
      <xsd:documentation xml:lang="en">The day on which the schedule becomes effective. Note: If the schedule category is 'weekly', then select a day of the week. If it is 'monthly' or 'yearly', then select a date in the month or the year respectively.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="開催期日-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="外部利用可否" type="ic:外部利用可否-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">施設や設備などの外部利用可否の記述のためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The textual description of whether or not a facility or equipment is available for use by outsiders.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="外部利用可否-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="概要" type="ic:概要-単純型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">概要説明を記述するためのプロパティ用語</xsd:documentation>
      <xsd:documentation xml:lang="en">The overview description.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="概要-単純型">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="カナ表記" type="ic:カナ表記-単純型">
    <xsd:annotation>
