      <xsd:element ref="ic:体重" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="名称型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">組織や地物等の名称を表現するためのクラス用語</xsd:documentation>
