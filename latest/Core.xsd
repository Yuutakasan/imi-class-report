  <xsd:complexType name="電話番号型">
    <xsd:annotation>
      <xsd:documentation xml:lang="ja">電話番号を表現するためのクラス用語 ※0-9 の数字及び + - ( ) , の文字のみを使用</xsd:documentation>
      <xsd:documentation xml:lang="en">A class term to express a telephone number. Note: Use '0 to 9' numeric characters and 'plus '+', minus '-', opening parenthesis '(', closing parenthesis ')', and comma ',' 'characters only.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="id" type="xsd:ID"/>
        <xsd:attribute name="ref" type="xsd:IDREF"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
