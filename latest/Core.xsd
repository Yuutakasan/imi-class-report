      <dct:license>http://creativecommons.org/publicdomain/zero/1.0/</dct:license>
      <dct:issued>2017-11-10</dct:issued>
      <dct:creator>
        <foaf:name xml:lang="ja">経済産業省</foaf:name>
        <foaf:name xml:lang="en">Ministry of Economy, Trade and Industry</foaf:name>
        <foaf:homepage>http://www.meti.go.jp/</foaf:homepage>
      </dct:creator>
