      <xsd:element ref="ic:カナ表記" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ic:ローマ字表記" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
